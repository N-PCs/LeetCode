class Solution {
public:
    vector<int> smallerNumbersThanCurrent(vector<int>& nums) {
        vector<int> freq(101, 0);
        
        for (int num : nums) {
            freq[num]++;
        }
        
        vector<int> prefix(101, 0);
        for (int i = 1; i < 101; i++) {
            prefix[i] = prefix[i-1] + freq[i-1];
        }
        
        vector<int> result;
        for (int num : nums) {
            result.push_back(prefix[num]);
        }
        
        return result;
    }
};
