class Solution {
public:
    int differenceOfSums(int n, int m) {
        int Msum,Nsum;
        Nsum=0;
        Msum=0;
        for(int i=1;i<=n;i++){
            if(i%m==0){
                Msum+=i;
            }
            else{
                Nsum+=i;
            }
        }
        return Nsum-Msum;
    }
};
